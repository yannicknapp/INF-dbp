package tirol_htl_anichstrasse;

import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;

import lombok.AccessLevel;
import lombok.Getter;
import lombok.Setter;

@Entity
@Getter
@Setter (AccessLevel.PUBLIC)

public class Bestellen {
	
	@Id
	@GeneratedValue (strategy = GenerationType.IDENTITY)
	private long id;
	private long kunde_id;
	private long adresse_rechnung_id;
	private long adresse_liefer_id;
	private long bestell_id;
	
	public Bestellen(int id,int k,int ar, int al, int bi) throws SQLException
	{
		
		this.id=id;this.kunde_id=k;this.adresse_rechnung_id=ar;this.adresse_liefer_id=al;this.bestell_id=bi;
	}
	
	public Bestellen()
	{
		
	}
	
	@Override
	public String toString()
	{
		return "Artikel [id=" + id + ", Bezeichnung=" + name + ", preis=" + preis +  "]";
	}

}
